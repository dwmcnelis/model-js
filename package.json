    "dist:test": "npm run test",
    "test": "npx run-s test:*",
    "test:eslint": "npx eslint src/js/**/*.js",
    "test:jest": "npx jest test/js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdmnco/model-js.git"
  },
